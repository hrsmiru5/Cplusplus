// Read a natural number $n$ from the keyboard. Calculate and display the arithmetic mean of all its divisors. The average must be displayed with exactly 2 decimal places.

#include <iostream>
#include <iomanip>
using namespace std;
int main()
{
    int n,d;
    float ma,nr=1,s;
    cin>>n;
    s=n;
    for(d=1; d<=n/2; d++)
    {
        if(n%d==0)
        {
            nr++;
            s=s+d;
        }
    }
    ma=(float)s/nr;
    
    cout<<fixed<<setprecision(2)<<ma;
    return 0;
}
